<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="861.0" prefWidth="1311.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.carrental.controllers.RentalController">

    <TableView fx:id="customerRentedTable" layoutX="119.0" layoutY="65.0" prefHeight="626.0" prefWidth="948.0">
        <columns>
            <TableColumn fx:id="customerID" prefWidth="100" text="Customer ID" />

            <TableColumn fx:id="customerName" prefWidth="150" text="Name" />
            <TableColumn fx:id="vehicleID" prefWidth="100" text="Vehicle ID" />

            <TableColumn fx:id="brand" prefWidth="150" text="Brand" />
         <TableColumn fx:id="rentedDate" prefWidth="150" text="Rented Date" />
         <TableColumn fx:id="returnDate" prefWidth="150" text="Return Date" />
         <TableColumn fx:id="rentalFee" prefWidth="150" text="Rental Fee" />
<!--         <TableColumn fx:id="colVehiclePic" prefWidth="150" text="Image" />-->
        </columns>
    </TableView>
   <Button layoutX="535.0" layoutY="713.0" mnemonicParsing="false" onAction="#returnVehicle" prefHeight="37.0" prefWidth="148.0" text="Returned">
      <font>
         <Font name="System Bold" size="14.0" />
      </font>
   </Button>
   <Label layoutX="562.0" text="Rentals">
      <font>
         <Font name="Times New Roman Bold" size="24.0" />
      </font>
   </Label>

</AnchorPane>
